{
    "name": "rentcars/ddd",
    "description": "Domain Driven Desing Example.",
    "keywords": ["ddd"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "league/container": "^2.4",
        "league/climate": "^3.2",
        "nesbot/carbon": "^1.22",
        "vlucas/phpdotenv": "^2.4",
        "catfan/Medoo": "^1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Domain\\": "domain/",
            "Infra\\": "infra/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
